    <script src="lib/script.js"></script>
    <script>
      // Add your code here

var hastaYasi = Number(document.getElementById("hastaYasi").value);
var hastaPh = Number(document.getElementById("hastaPh").value);
var pao2Degeri = Number(document.getElementById("pao2Degeri").value);
var paco2Degeri = Number(document.getElementById("paco2Degeri").value);
var hco3Degeri = Number(document.getElementById("hco3Degeri").value);



function aaHesap() {
  var pao2Degeri = Number(document.getElementById("pao2Degeri").value);
  var paco2Degeri = Number(document.getElementById("paco2Degeri").value);
  
  console.log("pao2Degeri:", pao2Degeri);
  console.log("paco2Degeri:", paco2Degeri);
  
  var hesaplananAa = 150 - (1.25 * paco2Degeri) - pao2Degeri;
  document.getElementById("hesaplananAa").value = hesaplananAa;
}

function aaYasaGore() {
  var hastaYasi = Number(document.getElementById("hastaYasi").value);
  var yasaGoreAa = ((hastaYasi+10)/4);
  document.getElementById("yasaGoreAa").value = yasaGoreAa;
}

function phYorumu() {
  var hastaPh = Number(document.getElementById("hastaPh").value);
  if (hastaPh>7.45) {
    document.getElementById("phYorumu").value = "ALKALOZ";
  } else if (hastaPh >=7.36 && hastaPh <=7.45) {
    document.getElementById("phYorumu").value = "NORMAL SINIRLARDA";
  } else if (hastaPh<7.35) {
    document.getElementById("phYorumu").value = "ASİDOZ";
  }
}

function kuralIkiYorumu() {
  var hastaPh = Number(document.getElementById("hastaPh").value);
  var paco2Degeri = Number(document.getElementById("paco2Degeri").value);
  if (hastaPh<=7.40 && paco2Degeri<40.0) {
    document.getElementById("kuralIkiYorumu").value = "Primer olay metabolik"
  } else if (hastaPh>7.40 && paco2Degeri>40.0) {
    document.getElementById("kuralIkiYorumu").value = "Primer olay metabolik"
  } else if (hastaPh<7.40 && paco2Degeri>40.0) {
    document.getElementById("kuralIkiYorumu").value = "Primer olay respiratuvar"
  } else if (hastaPh>7.40 && paco2Degeri<40.0) {
    document.getElementById("kuralIkiYorumu").value = "Primer olay respiratuvar"
  } else {
     document.getElementById("kuralIkiYorumu").value = "Primer olay yorumlanamaz"
  }
}

function kuralUcYorumu1() {
  var hastaPh = Number(document.getElementById("hastaPh").value);
  var paco2Degeri = Number(document.getElementById("paco2Degeri").value);
  if (hastaPh>7.35 || hastaPh<7.45) {
    if (paco2Degeri>40.0) {
      document.getElementById("kuralUcYorumu").value = "R.asidoz - M.alkaloz"
    } else if (paco2Degeri<40.0) {
      document.getElementById("kuralUcYorumu").value = "R.alkaloz - M.asidoz"
    }
  } else if (paco2Degeri>35.0 || paco2Degeri<45.0) {
    if (hastaPh>7.40) {
      document.getElementById("kuralUcYorumu").value = "M.alkoloz - R.asidoz"
    } else if (hastaPh<7.40) {
      document.getElementById("kuralUcYorumu").value = "M.asidoz - R.alkaloz"
    }
  }
}

function beklenenPaco2() {
  var hastaPh = Number(document.getElementById("hastaPh").value);
  var hco3Degeri = Number(document.getElementById("hco3Degeri").value);
  if (hastaPh<7.35) {
    document.getElementById("beklenenPaco2_1").value = (1.5 * hco3Degeri) + 8
  } else if (hastaPh>7.35) {
    document.getElementById("beklenenPaco2_2").value = (0.9 * hco3Degeri) + 15
  }
}

function beklenenHco3() {
  var hastaPh = Number(document.getElementById("hastaPh").value);
  var paco2Degeri = Number(document.getElementById("paco2Degeri").value);
  if (hastaPh<7.35 && paco2Degeri>40) {
    document.getElementById("beklenenHco3_1").value = (paco2Degeri - 40)/10
  } else if (hastaPh>7.45 && paco2Degeri<40) {
    document.getElementById("beklenenHco3_2").value = (40 - paco2Degeri)/5
  }
}

function agHesapla() {
  var naDegeri = Number(document.getElementById("naDegeri").value);
  var clDegeri = Number(document.getElementById("clDegeri").value);
  var hco3Degeri = Number(document.getElementById("hco3Degeri").value);
  var agDegeri = naDegeri - (hco3Degeri +clDegeri );
  document.getElementById("agDegeri").value = agDegeri;
  if (agDegeri<8.0) {
    document.getElementById("agYorumu").value = "Azalmış"
  } else if (agDegeri>16.0) {
    document.getElementById("agYorumu").value = "Artmış"
  } else {
    document.getElementById("agYorumu").value = "Normal aralıkta"
  }

  if (agDegeri>16.0) {
    var deltaDelta = (agDegeri-12)/(24-hco3Degeri)
    document.getElementById("deltaDelta").value = deltaDelta;

    if (deltaDelta<1.0) {
      document.getElementById("deltaDeltaYorumu").value = "+ hiperkloremik met.asidoz"
    } else if (deltaDelta>1.0) {
      document.getElementById("deltaDeltaYorumu").value = "+ met.alkaloz"
    }
  }


}

/*function tutarlilikKontrolu() {
  var hastaPhString = document.getElementById("hastaPh").value;
  var paco2Degeri = Number(document.getElementById("paco2Degeri").value);
  var hco3Degeri = Number(document.getElementById("hco3Degeri").value);
  var phParts = hastaPhString.split(".");
  var ondalikKisim =  phParts[1];
  var yeniDegisken = parseInt(ondalikKisim);
  var ilkDenklem = 24 * (paco2Degeri/hco3Degeri);
  var ikinciDenklem = 80 - yeniDegisken;
  if (ikinciDenklem == ilkDenklem ) {
    document.getElementById("tutarlilikYorumu").value = "TUTARLI"
  } else {
    document.getElementById("tutarlilikYorumu").value = "TUTARLI DEĞİL
  }
}*/


